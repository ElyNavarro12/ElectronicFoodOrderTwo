<!-- Encabezado !-->
<header class="baseBox-backend">

  <!-- Titulo !-->
  <div class="title-backend baseBox-backend">
    <span class="icon-backend icon-pacman"></span>
    <h1>OutBess</h1>
  </div>
  <!-- Opciones de navegacion !-->
  <div class="navBar-backend baseBox-backend">
    <!-- Cerrar sesion actual !-->
    <div class="sign_out-backend baseBox-backend">
      <span class="icon-backend icon-user-backend"></span>
      <% if user_signed_in? %>
        <%= link_to current_user.email, destroy_user_session_path, method: :delete  %>
      <% end %>
    </div>
    <!-- Navegacion !-->
    <div class="navigation-backend">
      <%= link_to '', '/config', :class => 'icon-menu' %>
    </div>
  </div>
</header>
